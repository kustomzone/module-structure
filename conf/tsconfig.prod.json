{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": false,
        "removeComments": true,
        "experimentalDecorators": true,
        "noImplicitReturns": true,
        "typeRoots": [
            "../node_modules/@types"
        ],
        "types": [
            "jquery",
            "node",
            "snapsvg"
        ]
    },
    "include": [
        "../src/**/*"
    ],
    "exclude": [
        "../node_modules/*"
    ]
}
